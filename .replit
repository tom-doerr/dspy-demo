modules = ["python-3.11", "python3", "nodejs-20"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DSPy Demo"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Minimal DSPy Demo"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simple DSPy Demo"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Super Simple DSPy Demo"

[[workflows.workflow]]
name = "DSPy Demo"
